package luiz05;
import java.util.Scanner;
public class seila {
   
    public static void seila(String[] args) {
        
        Scanner a = new Scanner(System.in);
        System.out.println("escreva o numero 1");
        int s = a.nextInt();
        
         
        System.out.println("escreva o numero 2");
        int d = a.nextInt();
        
        if (s == d) {
            System.out.println("os valores sao iguais");
        } else {
            System.out.println("os valores sao diferentes");
        }
        
     if (s >= d) {
            System.out.println("o valor 1 e maior");
        } else {
            System.out.println("o valor 2 e menor");
        }
        
        if (s < d) {
            System.out.println("o valor 1 e menor");
        } else {
            System.out.println("o valor 2 e maior");
        }
        
       
        
        
        
        
    }
    
    
    
}
