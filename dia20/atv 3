
package luiz05;
import java.util.Scanner;
public class seila3 {
    
     public static void seila3(String[] args) {

		Scanner a = new Scanner (System.in);
		System.out.println("digite o valor do carro ");
		double s=a.nextDouble();
		double d= (s * 0.28);
		double h= s*0.45;
		double k = s+d+h;
		System.out.println("O valor do carro vai ser de R$"+k);
	}

}
    
